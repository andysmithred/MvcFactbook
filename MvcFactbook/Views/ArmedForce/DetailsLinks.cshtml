@model MvcFactbook.ViewModels.Models.Main.ArmedForceView

<partial name="LinkDetails" model='new { Id = Model.Id }' />
<partial name="LinkItems" model='new {
    Id = Model.Id,
    Items = Model.Flags,
    Controller = "ArmedForce",
    Action = "DetailsFlagsList",
    Icon = "/images/icons/flag-light.png",
    Label = "Flags"
}' />
<partial name="LinkItems" model='new {
    Id = Model.Id,
    Items = Model.Branches,
    Controller = "ArmedForce",
    Action = "DetailsBranchesList",
    Icon = "/images/icons/branch-light.png",
    Label = "Branches"
}' />
