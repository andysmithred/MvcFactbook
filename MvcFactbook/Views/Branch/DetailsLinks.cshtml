@model MvcFactbook.ViewModels.Models.Main.BranchView

<partial name="LinkDetails" model='new { RouteId = Model.Id }' />
<partial name="LinkFlags" model='new { Controller = "Branch", RouteId = Model.Id, Items = Model.Flags }' />
<partial name="LinkShipServices" model='new { Controller = "Branch", RouteId = Model.Id, Items = Model.ShipServices }' />
<partial name="LinkArmedForce" model="Model.ArmedForce" />
<partial name="LinkBranchType" model="Model.BranchType" />
<partial name="LinkTotalFleet" model='new { Controller = "Branch", RouteId = Model.Id, Name = "Total Fleet" }' />
<partial name="LinkActiveFleet" model='new { Controller = "Branch", RouteId = Model.Id, Name = "Active Fleet" }' />
<partial name="LinkInactiveFleet" model='new { Controller = "Branch", RouteId = Model.Id, Name = "Inactive Fleet" }' />
