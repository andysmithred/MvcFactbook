@model MvcFactbook.ViewModels.Models.Main.BranchFlagView

<div class="card alert alert-dark rounded p-2 w-100">
    <div class="d-flex">
        <div class="p-2">
            <img src="~/images/icons/branch-dark.png" style="height:40px;width:40px"  />
        </div>
        <div class="p-2 pl-3">
            <h3><strong>@Model.Branch.Name</strong></h3>
        </div>
        <div class="p-2 pr-3 ml-auto">
            <h3><strong>@Model.Flag.Name</strong></h3>
        </div>
        <div class="p-1">
            <img class="card-img-top-50 rounded" src="@Model.Flag.ImageSource" alt="Card image" style="height:50px;object-fit: cover;overflow: hidden;opacity:0.3">
        </div>
    </div>
</div>