@model MvcFactbook.ViewModels.Models.Main.BranchTypeView

@{
    ViewData["Title"] = "Details";
}

<br />
<partial name="DetailsMenu" />
<br />
<partial name="DetailsHeader" />

<div class="card-deck">
    <div class="card border-0">
        <div class="card-header bg-secondary text-white">
            <div class="d-flex">
                <div><h5>Details</h5></div>
                <div class="ml-auto"><h5><i class="fas fa-info-circle"></i></h5></div>
            </div>
        </div>
        <div class="card-body bg-light rounded-bottom">
            <partial name="BasicLabel" model='new { Label = Html.LabelFor(x => x.Id), Value = Model.Id }' />
            <partial name="BasicLabel" model='new { Label = Html.LabelFor(x => x.Type), Value = Model.Type }' />
            <partial name="BasicLabel" model='new { Label = Html.LabelFor(x => x.Code), Value = Model.Code }' />
        </div>
    </div>
    <div class="card border-0">
        <div class="card-header bg-secondary text-white">
            <div class="d-flex">
                <div><h5>Branches</h5></div>
                <div class="ml-auto">
                    <h5><a asp-controller="Branch" asp-action="CreateByBranchType" asp-route-id="@Model.Id" style="color:white" title="Add Branch"><i class="fas fa-plus"></i></a></h5>
                </div>
            </div>
        </div>
        <div class="card-body bg-transparent">
           
        </div>
    </div>
</div>