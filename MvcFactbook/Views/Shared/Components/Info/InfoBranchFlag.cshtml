@model MvcFactbook.ViewModels.Models.Main.BranchFlagView

<div class="d-flex flex-row flex-wrap">
    <partial name="InfoItem" model='new { Label = Html.DisplayNameFor(x => x.Branch), Value = Model.Branch.Name }' />
    <partial name="InfoItem" model='new { Label = Html.DisplayNameFor(x => x.Flag), Value = Model.Flag.Name }' />
    <partial name="InfoItem" model='new { Label = Html.DisplayNameFor(x => x.Start), Value = Model.StartDateLabel }' />
    <partial name="InfoItem" model='new { Label = Html.DisplayNameFor(x => x.End), Value = Model.EndDateLabel }' />
</div>
