@model MvcFactbook.ViewModels.Models.Main.PoliticalEntityFlagView

<div class="d-flex flex-row flex-wrap">
    <partial name="InfoItem" model='new { Label = Html.DisplayNameFor(x => x.PoliticalEntity), Value = Model.PoliticalEntity.Name }' />
    <partial name="InfoItem" model='new { Label = Html.DisplayNameFor(x => x.Flag), Value = Model.Flag.Name }' />
    <partial name="InfoItem" model='new { Label = Html.DisplayNameFor(x => x.StartDate), Value = Model.StartDateLabel }' />
    <partial name="InfoItem" model='new { Label = Html.DisplayNameFor(x => x.EndDate), Value = Model.EndDateLabel }' />
</div>
