<div class="p-1 mr-2">
    <a asp-controller="@Model.Controller" asp-action="@Model.Action" asp-route-id="@Model.RouteId">
        <div class="d-flex">
            <div class="mr-2"><img src="@Model.Icon" alt="Card image" style="height:20px;width:20px;"></div>
            <div class="mr-2 text-white">@Model.Label</div>
            <div class="mr-2">
                <h5><i class='fas fa-angle-right text-white'></i></h5>
            </div>
        </div>
    </a>
</div>
