<div class="d-flex flex-row flex-wrap mt-2">
    @foreach (var item in Model)
    {
    <div class="pr-4 pl-2">
        <a asp-controller="ShipGroup" asp-action="Details" asp-route-id="@item.Id">
            <div class="d-flex flex-row">
                <div class="pt-2 pr-2">
                    <img class="rounded" src="@item.IconLightFullPath" alt="Card image" style="height:20px;width:20px;object-fit: cover;overflow: hidden;opacity:0.4" />
                </div>
                <div class="p-2">
                    <div class="text-white"><h5>@item.Name</h5></div>
                </div>
            </div>
        </a>
    </div>
    }
</div>
