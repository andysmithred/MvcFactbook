@model MvcFactbook.ViewModels.Models.Main.ShipView

<partial name="LinkDetails" model='new { RouteId = Model.Id }' />
<partial name="LinkShipServices" model='new { Controller = "Ship", RouteId = Model.Id, Items = Model.ShipServices }' />
<partial name="LinkBuilder" model="Model.Builder" />

@foreach (var item in Model.ShipSubTypes)
{
    <partial name="LinkShipSubType" model="item" />
}

@foreach (var item in Model.ShipClasses)
{
    <partial name="LinkShipClass" model="item" />
}

@foreach (var item in Model.Branches)
{
    <partial name="LinkBranch" model="item" />
}
