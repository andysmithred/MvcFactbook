@model MvcFactbook.ViewModels.Models.Main.ShipClassView

<div class="d-flex mt-3 p-2">
    <partial name="LinkDetails" model='new { Id = Model.Id }' />

    <partial name="LinkShips" model='new { Id = Model.Id, Items = Model.Ships }' />
    <partial name="LinkShipServices" model='new { Id = Model.Id, Items = Model.ShipServices }' />

    @foreach (var item in Model.ShipSubTypes)
    {
        <partial name="LinkShipSubType" model='new { Item = item }' />
    }

    @foreach (var item in Model.Branches)
    {
        <partial name="LinkBranch" model='new { Item = item }' />
    }
</div>
