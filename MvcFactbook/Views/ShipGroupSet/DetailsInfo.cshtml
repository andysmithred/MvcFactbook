@model MvcFactbook.ViewModels.Models.Main.ShipGroupSetView

<div class="p-2">
    <partial name="DetailsItemHeader" model='new { Label = "SHIP SERVICE", Icon = "/images/icons/shipservice-light.png" }' />
</div>
<div class="p-2">
    <partial name="InfoShipService" model=@Model.ShipService />
</div>
<hr class="bg-light" />

<div class="p-2">
    <partial name="DetailsItemHeader" model='new { Label = Model.ShipGroup.Name.ToUpper(), Icon = Model.ShipGroup.IconLightFullPath }' />
</div>
<div class="p-2 text-white">
    <span>@Model.ShipGroup.Description</span>
</div>
<hr class="bg-light" />
