@model MvcFactbook.ViewModels.Models.Main.SucceedingClassView

<div class="card alert alert-dark rounded p-2 w-100">
    <div class="d-flex">
        <div class="p-2">
            <img src="~/images/icons/shipclass-dark.png" style="height:40px;width:40px" />
        </div>
        <div class="p-2 pl-3">
            <h3><strong>@Model.PrecedingShipClass.Name</strong></h3>
        </div>
        <div class="p-2 pr-3 ml-auto">
            <h3><strong>@Model.SucceedingShipClass.Name</strong></h3>
        </div>
        <div class="p-1">
            <img src="~/images/icons/shipclass-dark.png" style="height:40px;width:40px" />
        </div>
    </div>
</div>